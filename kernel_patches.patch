+#define __IO_ADDRESS_INRANGE(x, base, size) ( (((x)>=(base)) && ((x)<((base)+(size))))&0x01 )
+#define __IO_ADDRESS_T(x, base, size, new) ( __IO_ADDRESS_INRANGE(x,base,size)*( (new) + ((x)-(base))) )
