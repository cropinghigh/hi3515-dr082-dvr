+
+	i2c_set_clientdata(i2c, tw2868);
+	tw2868->dev = dev;
+	
+	dev_set_drvdata(dev, tw2868);
+
+	tw2868->regmap = devm_regmap_init_i2c(i2c, &tw2868_regmap_config);
+	if (IS_ERR(tw2868->regmap))
